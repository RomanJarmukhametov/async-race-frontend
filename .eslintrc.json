{
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "airbnb-typescript",
    "prettier"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/jsx-single-quote": "off",
    "react/jsx-one-expression-per-line": "off",
    "typescript-eslint/quotes": ["off", "single"]
  }
}
